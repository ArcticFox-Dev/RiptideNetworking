{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "**.csproj"
          ],
          "src": "../RiptideNetworking/"
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadata": {
        "_appTitle": "Riptide Docs",
        "_appFooter": "Copyright Â© Tom Weiland",
        "_appLogoPath": "images/logo.png",
        "_appFaviconPath": "images/favicon.ico",
        "_enableNewTab": true,
        "_enableSearch": true,
        "_gitContribute": {
          "repo": "https://github.com/RiptideNetworking/Riptide",
          "branch": "main",
          "apiSpecFolder": "Docs/apidoc"
        }
    },
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "plugins/memberpage.2.59.0/content",
      "templates/riptide"
    ],
    "xrefService": [
      "https://xref.docs.microsoft.com/query?uid={uid}"
    ],
    "sitemap": {
      "baseUrl": "https://riptidenetworking.github.io/Riptide/"
    },
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}